<!-- <!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 
	2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd" > -->

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<description>
		</description>
		<display-name>
		RegisterLeague
	</display-name>
		<servlet-name>RegisterLeague</servlet-name>
		<servlet-class>com.trainingbasket.javaee.web.RegisterLeague</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>ProcessFile</display-name>
		<servlet-name>ProcessFile</servlet-name>
		<servlet-class>com.trainingbasket.javaee.web.ProcessFile</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterLeague</servlet-name>
		<url-pattern>/registerleague</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProcessFile</servlet-name>
		<url-pattern>/processFile</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	
	<listener>
		<listener-class>com.trainingbasket.javaee.utils.InitListner</listener-class>
	</listener>
</web-app>
